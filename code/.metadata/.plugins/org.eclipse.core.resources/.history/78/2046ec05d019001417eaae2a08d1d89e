package ch.desm.middleware.modules.communication.endpoint.serial.ubw32;

public abstract class EndpointUbw32Base {

	/**
	 * 
	 * @param port
	 * @param pin
	 * @param value
	 */
	public void setPinOutputDigital(String port, String pin, String value);

	/**
	 * 
	 * @param port
	 * @param pin
	 * @param value
	 */
	public void getPinInputDigital(String port, String pin);
	
	/**
	 * 
	 * @param port
	 * @param pin
	 * @param value
	 */
	public void getPinInputAnalog();
}
