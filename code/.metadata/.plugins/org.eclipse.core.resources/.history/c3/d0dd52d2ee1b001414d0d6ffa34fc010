package ch.desm.middleware.modules.communication.endpoint.virtual;

import org.apache.log4j.Logger;

class InterlockingPetriNetBase {

	protected String firedElement;
	
	protected static Logger log = Logger.getLogger(InterlockingPetriNetBase.class);
	
	protected boolean canFire(String s) {
		firedElement = s;
		return true;
	}
	
	public String getFiredElement(){
		return firedElement;
	}
	
	
}
