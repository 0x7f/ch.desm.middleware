package ch.desm.middleware.modules.component.interlocking.obermattlangnau;

import org.apache.log4j.Logger;

public class EndpointInterlockingPetriNetBase {

	protected String firingElement;
	
	protected static Logger log = Logger.getLogger(OMLPetriNetBase.class);
	
	protected boolean canFire(String s) {
		firingElement = s;
		return true;
	}
	
	public String getFiredElement(){
		return firingElement;
	}
	
	public void fire(String s) {
		System.out.println(s);
	}
	
	
}
