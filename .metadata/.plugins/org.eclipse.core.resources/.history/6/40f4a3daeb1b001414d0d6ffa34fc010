package ch.desm.middleware.modules.communication.endpoint.virtual;

import org.apache.log4j.Logger;

public class PetrinNetBase {

	public String firingElement;
	
	public static Logger log = Logger.getLogger(PetrinNetBase.class);
	
	public boolean canFire(String s) {
		firingElement = s;
		return true;
	}
	
}
